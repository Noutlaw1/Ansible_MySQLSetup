CREATE DATABASE `webapp` /*!40100 COLLATE 'latin1_swedish_ci' */
CREATE TABLE `webapp_logins` (
	`TIMESTAMP` TIMESTAMP NULL,
	`ipaddress` VARCHAR(15) NULL DEFAULT NULL
)
COLLATE='latin1_swedish_ci'
;

CREATE USER 'webappuser'@'localhost' IDENTIFIED BY 'Password1!';
GRANT ALTER, SHOW VIEW, SHOW DATABASES, SELECT, PROCESS, EXECUTE, CREATE ROUTINE, CREATE, ALTER ROUTINE, CREATE VIEW, CREATE TEMPORARY TABLES, CREATE TABLESPACE, EVENT, DROP, DELETE, REFERENCES, INSERT, INDEX, CREATE USER, UPDATE, TRIGGER, LOCK TABLES, FILE, REPLICATION SLAVE, REPLICATION CLIENT, RELOAD, SUPER, SHUTDOWN  ON *.* TO 'webappuser'@'localhost' WITH GRANT OPTION;
FLUSH PRIVILEGES;

CREATE USER 'testuser'@'localhost' IDENTIFIED BY 'Password1!';
